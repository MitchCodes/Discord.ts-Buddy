{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "linebreak-style": 0,
        "prefer-const": 0,
        "prefer-template": 0,
        "no-debugger": 0,
        "@typescript-eslint/no-inferrable-types": 0
    }
  }